<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="default" namespace="/" extends="struts-default">
        <action name="RegistConfirmAction" class="com.diworksdev.account.action.RegistConfirmAction">
            <result name="input">/regist.jsp</result>
            <result name="success">/regist_confirm.jsp</result>
        </action>

        <action name="RegistCompleteAction" class="com.diworksdev.account.action.RegistCompleteAction">
            <result name="success">/regist_complete.jsp</result>
        </action>

        <action name="UserCreateCompleteAction" class="com.diworksdev.account.action.UserCreateCompleteAction">
            <result name="success">/home.jsp</result>
        </action>

        <action name="RegistAction" class="com.diworksdev.account.action.RegistAction">
            <result name="input">/regist.jsp</result>
            <result name="success">/regist_confirm.jsp</result>
        </action>
    </package>
</struts>
